n=int(input())
num=int(input())
largest=num
small=num
i=1
while(i<n):
    num=int(input())
    if(num>largest):
        largest=num
    elif(num<small):
        small=num
    i=i+1
print("largest=",largest)
print("smallest=",small)
diff=largest-small
print("Difference of largest and smallest=",diff)

