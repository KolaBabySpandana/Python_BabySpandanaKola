// method 1
def year(y):
    if y%4==0 and y%100!=0 or y%400==0:
        print("Leap year")
    else:
        print("Not Leap year")
y=int(input())
year(y)


// method 2
def year(y):
    if y%4==0 and y%100!=0 or y%400==0:
        print("Leap year")
    else:
        print("Not Leap year")
y=int(input())
print(year(y))

//method 3
def year(a):
    if y%4==0 and y%100!=0 or y%400==0:
        return True
    else:
        return False
y=int(input())
if year(y):
    print("Leap year")
else:
    print("Not Leap year")
