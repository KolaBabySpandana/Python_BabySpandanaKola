n=int(input())
a=list(map(int,input().split()))
check=0
v=int(input())
for i in range(0,n):
    for j in range(i+1,n):
        if a[i]-a[j]==v:
            check=1
            break
if check==1:
    print("yes")
else:
    print("no")
    
output
4
45 50 45 65
5
yes

or

n=int(input())
a=list(map(int,input().split()))
check=0
v=int(input())
for i in range(0,n):
    for j in range(i+1,n):
        if a[i]-a[j]==v:
            check=1
            print(i,j)
            break
if check==0:
    print("-1")
    
output
4
7 3 2 89
1
1 2
